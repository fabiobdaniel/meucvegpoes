{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://awakenyourhero.com.br/:path*",
      "permanent": true,
      "has": [
        { "type": "host", "value": "fabiobdaniel.com" }
      ]
    },
    {
      "source": "/:path*",
      "destination": "https://awakenyourhero.com.br/:path*",
      "permanent": true,
      "has": [
        { "type": "host", "value": "www.fabiobdaniel.com" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

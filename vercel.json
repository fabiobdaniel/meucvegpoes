{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://awakenyourhero.com.br/:path*",
      "permanent": true,
      "has": [
        { "type": "host", "value": "fabiobdaniel.com" }
      ]
    },
    {
      "source": "/:path*",
      "destination": "https://awakenyourhero.com.br/:path*",
      "permanent": true,
      "has": [
        { "type": "host", "value": "www.fabiobdaniel.com" }
      ]
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
